            <ref name="wega_data.count"/>
            <optional>
              <ref name="wega_data.count"/>
            </optional>
          </list>
        </attribute>
      </optional>
      <optional>
        <attribute name="writtenLines">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the number of written lines per
      column</a:documentation>
          <list>
            <ref name="wega_data.count"/>
            <optional>
              <ref name="wega_data.count"/>
            </optional>
          </list>
        </attribute>
      </optional>
