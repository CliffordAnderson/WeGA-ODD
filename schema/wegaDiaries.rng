        <zeroOrMore>
          <ref name="wega_model.headLike"/>
        </zeroOrMore>
        <choice>
          <oneOrMore>
            <ref name="wega_model.pLike"/>
          </oneOrMore>
          <group>
            <optional>
              <ref name="wega_msContents"/>
            </optional>
            <optional>
              <ref name="wega_physDesc"/>
            </optional>
            <optional>
              <ref name="wega_history"/>
            </optional>
            <optional>
              <ref name="wega_additional"/>
            </optional>
            <zeroOrMore>
              <ref name="wega_msPart"/>
            </zeroOrMore>
          </group>
        </choice>
      </group>
      <ref name="wega_att.global.attributes"/>
      <empty/>
    </element>
