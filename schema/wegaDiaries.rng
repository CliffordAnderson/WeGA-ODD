        <group>
          <optional>
            <ref name="wega_summary"/>
          </optional>
          <oneOrMore>
            <ref name="wega_layout"/>
          </oneOrMore>
        </group>
      </choice>
      <ref name="wega_att.global.attributes"/>
      <empty/>
