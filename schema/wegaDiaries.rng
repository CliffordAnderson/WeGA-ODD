        <oneOrMore>
          <choice>
            <ref name="wega_model.pLike"/>
            <ref name="wega_decoNote"/>
            <ref name="wega_condition"/>
          </choice>
        </oneOrMore>
        <oneOrMore>
          <ref name="wega_binding"/>
        </oneOrMore>
      </choice>
      <ref name="wega_att.global.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="wega_binding">
    <element name="binding">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a description of one binding, i.e. type of covering, boards,
    etc. applied to a manuscript. [10.7.3.1. ]</a:documentation>
