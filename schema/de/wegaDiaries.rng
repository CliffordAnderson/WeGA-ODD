        <group>
          <ref name="abbr"/>
          <ref name="expan"/>
        </group>
        <group>
          <ref name="unclear"/>
          <oneOrMore>
            <ref name="unclear"/>
          </oneOrMore>
        </group>
      </choice>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="choice-constraint-multipleUnclear">
        <rule context="tei:choice">
          <sch:report xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" test="(count(./tei:unclear) &gt; 1) and (count(./tei:unclear[@cert='high']) != 1)">When working with several unclear, accurately one has to marked with a cert="high".</sch:report>
        </rule>
      </pattern>
